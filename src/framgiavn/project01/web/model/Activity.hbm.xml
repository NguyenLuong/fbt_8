<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="framgiavn.project01.web.model.Activity" table="Activity">
    <id name="id">
      <generator class="assigned"></generator>
    </id>

    <property name="targetId" type="integer" column="target_id"></property>
    <property name="targetName" column="target_name"></property>
    <property name="actionName" column="action_name"></property>
    <many-to-one name="userId" class="framgiavn.project01.web.model.User" column="user_id"
      not-null="true" />
  </class>

</hibernate-mapping>
