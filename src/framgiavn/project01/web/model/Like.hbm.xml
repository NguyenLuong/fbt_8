<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="framgiavn.project01.web.model.Like" table="Like">
    <id name="id">
      <generator class="assigned"></generator>
    </id>
    <many-to-one name="userId" class="framgiavn.project01.web.model.User" column="user_id"
      not-null="true" />
    <many-to-one name="reviewId" class="framgiavn.project01.web.model.Review" column="review_id"
      not-null="true" />  
  </class>
</hibernate-mapping>
